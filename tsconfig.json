{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "lib": [
      "es6",
      "dom"
    ],
    "module": "es6",
    "target": "es5",
    "types": [
      "reflect-metadata"
    ],
    "jsx": "react",
    "allowJs": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}